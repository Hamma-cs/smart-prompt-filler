{
  "manifest_version": 3,
  "name": "Smart Prompt Filler - V4",
  "version": "1.0",
  "description": "ملء تلقائي مباشر لنماذج الذكاء الاصطناعي بقوالب محددة مسبقاً",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting" 
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon48.png"
  }
}